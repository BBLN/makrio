<div class="content">
    <img src="{{screenshot_url}}" {{#if adjustedImageHeight}}style="height:{{adjustedImageHeight}}px;"{{/if}} class="frame-screenshot" />

    <!--Handlebars partial territory-->
    <div class=controls>
        <a href="#" class="delete"></a>
    </div>

    <div class="info permalink">
        {{#if parent}}
        {{#linkToPerson parent.author}}
        {{{personImage this}}
        {{/linkToPerson}}
        {{/if}}

        <i class="icon-time timestamp" title="{{created_at}}" rel="tooltip"></i>
        <i class="icon-chevron-right permalink" title="View Post" rel="tooltip"></i>

        <i class="icon-heart"></i> {{likesCount}}
        <i class="icon-random"></i> {{resharesCount}}
        <i class="icon-comment"></i> {{commentsCount}}
    </div>
</div>